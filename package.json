{
  "name": "sec-incentive",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx api/server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "prisma generate && tsc -b && vite build",
    "preview": "vite preview",
    "start": "tsx api/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "postinstall": "prisma generate",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/multer": "^2.0.0",
    "@types/node": "^20.16.10",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.47",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5",
    "vite": "^5.4.8"
  }
}
